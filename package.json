{
  "name": "@h7ml/badwords",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@7.12.0",
  "description": "仅限用于合法的、积极向上的敏感词过滤使用，严禁用于从事违反法律法规、危害国家、危害人民、不道德的活动！！！",
  "author": "h7ml <h7ml@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/h7ml",
  "homepage": "https://github.com/h7ml/badwords#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h7ml/badwords.git"
  },
  "bugs": "https://github.com/h7ml/badwords/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "commit": "git-cz",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "words": "esno src/words/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@antfu/ni": "^0.20.0",
    "@antfu/utils": "^0.7.2",
    "@types/node": "^18.14.1",
    "bumpp": "^9.0.0",
    "cz-git": "^1.5.3",
    "eslint": "^8.34.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.1.2",
    "pnpm": "^7.33.4",
    "rimraf": "^4.1.2",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.4",
    "vitest": "^0.28.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
}
